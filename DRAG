			MeshType = "Sphere",
			Scale = Vector3.new(1, 1, 1)
		})
		TweenService:Create(mesh, TweenInfo.new(1), {
			Scale = Vector3.new(30, 30, 30)
		}):Play()
		TweenService:Create(orb, TweenInfo.new(1), {Transparency = 1}):Play()
		Hair.Mesh.MeshId = "rbxassetid://430344159"
